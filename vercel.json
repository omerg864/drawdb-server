{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      }
    ],
    "rewrites": [
      {
          "source": "/(.*)",
          "destination": "dist/index.js"
      }
    ]
  }